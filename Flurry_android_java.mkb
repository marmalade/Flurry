#!/usr/bin/env mkb
# Builder mkb file for the java portion of the Flurry extension on android
platform JAVA

files
{
    (source/android)
    Flurry.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "flurry/android/Flurry_3.2.2.jar"

option output-name=lib/android/Flurry.jar
